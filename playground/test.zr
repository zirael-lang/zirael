enum Option<T> {
    Some { value: T },
    None,

    fn unwrap(self) {}
}

struct Person {
    age: int,

    fn birthday(self) {
        self.age += 1;
    }
}

fn main() {
    var _some = Option::Some(10);
    var _sd = Option::Some(Person { age: 25 });
    Option::Some("test").unwrap();
    var _none = Option::None;
    var bob = Person { age: 45 };

    bob.birthday();
}