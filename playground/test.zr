enum Result<T, E> {
    Ok { value: T },
    Err { error: E },
}

fn divide(a: int, b: int): Result<int, string> {
    b == 0 ? Result::Err("division by zero") : Result::Ok(a / b)
}

fn main() {
    var _result = divide(10, 2);

    var _res = match divide(10, 0) {
        Result::Ok { value } => {
            value
        },
        Result::Err { error } => {
            error
        }
    };
}