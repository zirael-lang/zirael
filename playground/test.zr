enum Option<T> {
    Some { value: T },
    None,

    fn unwrap(self) {}
}

struct Person {
    age: int
}

fn main() {
    var _some = Option::Some(10);
    var _sd = Option::Some(Person { age: 25 });
    var _sw = Option::Some("test").unwrap();
    var _none = Option::None;
    var _person = Person { age: 45 };
}